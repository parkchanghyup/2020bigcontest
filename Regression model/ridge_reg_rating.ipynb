  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {
    "ExecuteTime": {
     "end_time": "2020-09-01T06:16:06.951285Z",
     "start_time": "2020-09-01T06:16:06.941298Z"
